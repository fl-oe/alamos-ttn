  "author": "Florian Eder",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.7.2",
    "chokidar": "^3.5.3",
    "dotenv": "^16.3.1",
    "fs-extra": "^11.1.1",
    "moment": "^2.29.4",
    "mqtt": "^5.8.0",
    "typescript": "^5.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "ts-node": "^10.9.1"
  }
