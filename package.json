  "version": "1.0.0",
  "description": "Application to get position data from the things network and save them to FE2 Alamos server.",
  "private": true,
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc --build",
    "start": "node ./dist/index.js",
    "format": "prettier --write \"**/*.ts\" \"**/*.json\"",
    "format-check": "prettier --check \"**/*.ts\" \"**/*.json\""
